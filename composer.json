{
    "name": "moveodev/moveo-aircall-php",
    "description": "Aircall API client built on top of Guzzle 7",
    "keywords": ["aircall", "aircall.io", "api", "guzzle"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "AntoineLemaire",
            "homepage": "https://github.com/AntoineLemaire"
        }
    ],
    "autoload": {
        "psr-4": {
            "Aircall\\": ["src"]
        }
    },
    "require": {
        "php": ">= 7.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "config": {
        "bin-dir": "bin",
        "apcu-autoloader": true
    }
}
